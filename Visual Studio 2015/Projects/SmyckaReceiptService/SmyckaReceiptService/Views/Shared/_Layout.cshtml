

<!DOCTYPE html>
<!-- <partial name="_CopyrightHeader" />-->
<html lang="en">
<head>
    <title>
        @RenderSection("Title", required: false)
    </title>
    <meta name="description" content="Receipt Service">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no, user-scalable=no, minimal-ui">
    <!-- Call App Mode on ios devices -->
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <!-- Remove Tap Highlight on Windows Phone IE -->
    <meta name="msapplication-tap-highlight" content="no">
    <!-- base css -->
    <link rel="stylesheet" media="screen, print" href="~/content/vendors.bundle.css">
    <link rel="stylesheet" media="screen, print" href="~/content/app.bundle.css">
    <link rel="stylesheet" media="screen, print" href="~/content/theme-demo.css">
    <link rel="stylesheet" media="screen, print" href="~/content/site.css">
    <link rel="stylesheet" media="screen, print" href="~/content/fa-brands.css">
    <link rel="stylesheet" media="screen, print" href="~/content/fa-solid.css">
    <!-- Place favicon.ico in the root directory -->
    <partial name="_FavIcon" />
    @RenderSection("Header", required: false)
</head>
<body class="mod-bg-1">
    <partial name="_ScriptsLoadingSaving" />
    <div class="page-wrapper">
        <div class="page-inner">
            <div class="page-content-wrapper">
                <main id="js-page-content" role="main" class="page-content">
                    @RenderSection("PageHeading", required: false)
                    @RenderBody()
                </main>
                <partial name="_PageFooter" />
            </div>
        </div>
    </div>
    <partial name="_ScriptBasePlugins" />
    @RenderSection("Scripts", required: false)
</body>
</html>